    private void A1ActionPerformed(java.awt.event.ActionEvent evt) {                                   
        // TODO add your handling code here:
        clicksPerTurn[numberClicksPerTurn][0] = 1;
        clicksPerTurn[numberClicksPerTurn][1] = 1;
        numberClicksPerTurn++;
        checkMove();
    }                                  

